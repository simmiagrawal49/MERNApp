{
  "name": "studioandstore",
  "version": "1.0.0",
  "description": "web service",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix ../frontend",
    "dev": "concurrently \"npm run start\" \"npm run client\" --ignore-client",
    "data:import": "node seeder.js",
    "data:destroy": "node seeder.js -d",
    "build": "npm install && npm install --prefix ../frontend && npm run build --prefix ../frontend"
  },
  "author": "simmi",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.4",
    "multer": "^1.4.5-lts.1",
    "razorpay": "^2.9.4",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22"
  }
}
